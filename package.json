{
  "name": "@beyonk/gdpr-cookie-consent-banner",
  "version": "15.0.4",
  "description": "GDPR Cookie Consent Banner for Svelte/Kit",
  "author": "Antony MacKenzie-Jones",
  "license": "MIT",
  "main": "src/lib/index.js",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build --config vite.components.config.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "eslint",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src/lib",
    "dist"
  ],
  "exports": {
    ".": {
      "svelte": "./src/lib/Banner.svelte",
      "import": "./src/lib/Banner.svelte"
    },
    "./banner.js": {
      "import": "./src/lib/index.js"
    },
    "./banner.css": {
      "import": "./src/lib/banner.css"
    }
  },
  "devDependencies": {
    "@beyonk/eslint-config": "^9.0.3",
    "@sveltejs/kit": "^2.20.7",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "eslint": "^9.23.0",
    "svelte": "^5.25.3",
    "vite": "^6.3.3",
    "vitest": "^3.1.2"
  },
  "type": "module",
  "keywords": [
    "gdpr",
    "cookie consent",
    "banner",
    "data protection",
    "eu",
    "consent",
    "cookie",
    "svelte",
    "vanilla",
    "beyonk"
  ],
  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "js-cookie": "^3.0.5",
    "uid": "1"
  }
}
