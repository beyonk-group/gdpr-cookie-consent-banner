{
  "name": "@beyonk/gdpr-cookie-consent-banner",
  "version": "12.1.2",
  "description": "GDPR Cookie Consent Banner for Svelte/Kit",
  "author": "Antony Jones",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev --host",
    "build": "vite build --config vite.components.config.js",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "eslint .",
    "test": "mocha \"src/**/*.spec.js\"",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src/lib",
    "dist"
  ],
  "exports": {
    ".": {
      "svelte": "./src/lib/Banner.svelte",
      "import": "./src/lib/Banner.svelte"
    },
    "./banner.js": {
      "import": "./dist/index.js"
    },
    "./banner.css": {
      "import": "./src/lib/banner.css"
    }
  },
  "dependencies": {
    "js-cookie": "^3.0.1"
  },
  "devDependencies": {
    "@beyonk/eslint-config": "^7.1.1",
    "@hapi/code": "^9.0.3",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.4",
    "@sveltejs/vite-plugin-svelte": "^2.4.4",
    "eslint": "^8.50.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "mocha": "^10.2.0",
    "svelte": "^4.0.5",
    "svelte-check": "^3.5.2",
    "vite": "^4.4.2"
  },
  "type": "module",
  "keywords": [
    "gdpr",
    "cookie consent",
    "banner",
    "data protection",
    "eu",
    "consent",
    "cookie",
    "svelte",
    "vanilla",
    "beyonk"
  ]
}
